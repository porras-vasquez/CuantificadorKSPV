            <div class="col-3">
                <div class="mb-2">
                    <label class="form-label">Ubicacion</label>
                    <input  type="text" name="ubicacion" class="form-control" value="<%=airConditioning.ubicacion%>" required/>
                </div>
                <div class="mb-2">
                    <label for="exampleInputEmail1" class="form-label">Serie</label>
                    <input  type="text" name="serie" class="form-control" value="<%=airConditioning.serie%>" required/>
                </div>
                <div class="mb-2">
                    <label for="exampleInputEmail1" class="form-label">Marca</label>
                    <input  type="text" name="marca" class="form-control" value="<%=airConditioning.marca%>" required/>
                </div>
                <div class="mb-2">
                    <label for="exampleInputEmail1" class="form-label">Modelo</label>
                    <input  type="text" name="modelo" class="form-control" value="<%=airConditioning.modelo%>" required/>
                </div>
                <div class="mb-2">
                    <label for="exampleInputEmail1" class="form-label">Capacidad</label>
                    <input  type="text" name="capacidad" class="form-control" value="<%=airConditioning.capacidad%>" required/>
